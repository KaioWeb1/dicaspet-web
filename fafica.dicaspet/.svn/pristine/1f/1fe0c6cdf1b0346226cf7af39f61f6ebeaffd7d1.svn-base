<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
          "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html
xmlns="http://www.w3.org/1999/xhtml"
xmlns:h="http://java.sun.com/jsf/html"
xmlns:ui="http://java.sun.com/jsf/facelets"
xmlns:f="http://java.sun.com/jsf/core"
xmlns:p="http://primefaces.org/ui">


<h:head>

<title>DicasPet - Celtic Solutions</title>
<script type="text/javascript">

function repor(){
	alert("Reposição realizada com sucesso!");
}

</script>
</h:head>
<h:body>
<h:outputStylesheet library="css" name="style.css"/>

	<div class="wrapper">
		<div class="header">
			<div class="topo">
				
					<div class="logo">
						<img src="../images/logodicaspet.png" class="img-logo"/>

					</div>
					<div class="menu">
						<ul class="ul-menu">
							<li><a href="../index.xhtml">Home</a></li>
							<li><a href="../servicos.xhtml">Serviços</a></li>
							<li><a href="../perguntas.xhtml">Perguntas e Respostas</a></li>
							<li><a href="../contato.xhtml">Contato</a></li>
						</ul>
					</div>
					<div class="busca">
						<ul>
							<li><input type="text" value="Procurar..."/><input
								type="submit" value="ok"/><img src=""/></li>
						</ul>
					</div>
	
			</div>

		</div>
		<!-- Conteudo do site começa apartir daqui -->
		<div class="geral">
				<!-- aqui é onde fica o conteúdo -->
				<!-- o layout do site divide-se em 3 colunas: left, center, right -->
				<div class="left">
					<div class="usuario">

						<img alt="" src="../images/user.jpg"/>
						<h3><h:outputLabel value="#{usuarioMB.user.usu_nome}"/></h3>
						<!-- <a href="../index.xhtml" class="bt-logoff">Sair</a> -->
						<h:form>
						<h:commandButton value="Sair" class="bt-logoff" action="#{usuarioMB.logoff}"/>
						</h:form>
						<div class="my-rank">
						<p>Posição atual</p>
						<img alt="" src="../images/rank/prata.png"/>
						<a href="#">2º Lugar</a>
												
						</div>
						<ul class="my-menu">
							<li><a href="lista-prestador.xhtml"><img alt="" src=""/>Listar Empresas</a></li>
							<li><a href="lista-servicos.xhtml"><img alt="" src=""/>Listar Serviços</a></li>
							<li><a href="lista-usuarios.xhtml"><img alt="" src=""/>Listar Usuários</a></li>							
						</ul>

					</div>

				</div>
				<div class="center">
								
					<!-- aqui é onde fica o conteúdo -->
				<!-- aqui é onde fica o conteúdo -->
					<div class="bloco-empresa">
						<h3>Lista Prestador</h3>
					<!-- 	
						<ul class="my-menu-lista">
						
							<li><a href="/fafica.dicaspet/admin/cadastro-animal.xhtml">Cadastrar Serviço</a></li>
							<li><a href="/fafica.dicaspet/admin/editar-animal.xhtml">Editar Serviço</a></li>
							<li><a href="#">Excluir Serviço</a></li>							
						</ul>
 					-->	
 					<br></br>
					<br></br>
					<div class="pergunta">
						<div class="link-resposta"><a href="cadastro-prestador.xhtml">Cadastrar Prestador</a></div>
					</div>
					
					<br></br>
					<br />

						<h:form id="listaPrestadoresAdm">
						<p:dataTable id="checkboxDT" var="prestador"
							value="#{prestadorMB.prestadorServicos}"
							selection="#{prestadorMB.prestadoresSelecionados}" rowKey="#{prestador.prtsrv_id}"
							style="margin-bottom:0" rows="10"
                         paginator="true"
                         paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
                         rowsPerPageTemplate="5,10,15">
							<f:facet name="header">
          Empresas
                            </f:facet>
							<p:column selectionMode="multiple"
								style="width:10%;text-align:center" />
							<p:column headerText="Nome">
								<h:outputText value="#{prestador.prtsrv_nome}" />
							</p:column>
							<p:column headerText="Tipo">
								<h:outputText value="##{prestador.prtsrv_tipo}}" />
							</p:column>
							<p:column headerText="cnpj">
								<h:outputText value="#{prestador.prtsrv_cnpj}" />
							</p:column>
							
							<f:facet name="footer">
							
							<!--  -->
								<p:commandButton process="checkboxDT"
									 icon="ui-icon-search"
									value="Detalhes" oncomplete="PF('multiCarDialog').show()" update=":listaPrestadoresAdm:multiPrestadorDetalhe" />
							</f:facet>
						</p:dataTable>

						<p:dialog header="Informações do pretador de serviços" widgetVar="carDialog" modal="true"
							showEffect="fade" hideEffect="fade" resizable="false">
							<p:outputPanel id="carDetail" style="text-align:center;">
								<p:panelGrid columns="2"
									rendered="#{not empty prestadorMB.prestadorSelecionado}"
									columnClasses="label,value">
									<f:facet name="header">
										<p:graphicImage
											name="demo/images/car/#{dtSelectionView.selectedCar.brand}-big.gif" />
									</f:facet>

									<h:outputText value="Id:" />
									<h:outputText value="#{prestadorMB.prestadorSelecionado.prtsrv_id}" />

									<h:outputText value="Nome" />
									<h:outputText value="#{prestadorMB.prestadorSelecionado.prtsrv_nome}" />

								</p:panelGrid>
							</p:outputPanel>
						</p:dialog>

						<p:dialog header="Prestadores selecionados" widgetVar="multiCarDialog"
							modal="true" showEffect="fade" hideEffect="fade"
							resizable="false" width="400">
							<p:outputPanel id="multiPrestadorDetalhe" style="text-align:center;">
								<ui:repeat value="#{prestadorMB.prestadoresSelecionados}" var="car">
									<h:outputText value="#{car.prtsrv_id} - #{car.prtsrv_nome}"
										style="display:block" />
										<h:commandButton value="Deletar" action="#{prestadorMB.deletar(car)}" />
										<h:commandButton value="Editar" action="#{prestadorMB.editarPrestadorRedirect(car)}" />
								</ui:repeat>
							</p:outputPanel>
						</p:dialog>
					</h:form>


				</div>
					<!-- Fim do Bloco -->

					<div class="banner">
						<h3>Publicidade</h3>
						<img alt="" src="../images/banner1.jpg" />
					</div>





				</div>
				<div class="right">

				<div class="banner">
					<h3>Adote um Pet</h3>
					<a href="admin/adotar.xhtml"><img alt=""
						src="../images/adote-um-melhor-amigo-1.jpg" /></a>
				</div>
				<div class="ranking-user">
					<h3>Ranking de Usuários</h3>
					<ui:repeat var="usuario" value="#{rankingMB.usuarios}">

						<div class="rank-item">
							<img alt="" src="../images/rank/ouro.png" />
							<h:outputText value="#{usuario.usu_nome}" />
							-
							<h:outputText value="#{usuario.usu_pontuacao}" />
							pontos
						</div>

					</ui:repeat>
				</div>


				<div class="ranking-serv">
					<h3>Ranking de Serviços</h3>
					<ui:repeat var="servico" value="#{rankingMB.servicos}">

						<div class="rank-item">
							<img alt="" src="../images/rank/ouro.png" />
							<h:outputText value="#{servico.srv_descricao}" />
							-
							<h:outputText value="#{servico.srv_pontuacao}" />
							pontos
						</div>

					</ui:repeat>
				</div>

				<div id="verifica-peso" class="verifica-peso">
					<h3>Verificar o Peso Ideal do cachorro</h3>
					<h:form>
						<br />
						<h:inputText value="#{pesoIdealMB.pesoAtual}" />
						<br /><br />
						<h:selectOneMenu value="#{pesoIdealMB.porte}">
							<f:selectItem itemValue="pequeno" itemLabel="Pequeno" />
							<f:selectItem itemValue="medio" itemLabel="Medio" />
							<f:selectItem itemValue="grande" itemLabel="Grande" />
						</h:selectOneMenu>
						<br /><br />
						<h:selectOneMenu value="#{pesoIdealMB.sexo}">
							<f:selectItem itemValue="femea" itemLabel="Femea" />
							<f:selectItem itemValue="macho" itemLabel="Macho" />
						</h:selectOneMenu>
						<br /><br />
						<h:commandButton value="Calcular"
							action="#{pesoIdealMB.calcularPesoIdeal}"  />
					</h:form>
					<br />
					<p class="msg-peso"><h:outputText value="#{pesoIdealMB.msg}" /></p>

				</div>
				<!-- 				<div class="ranking-user">
					<h3>Ranking de Usuários</h3>

					<div class="rank-item">
						<img alt="" src="images/rank/ouro.png" /> - <a href="#">Jão
							da Silva - 10429 pontos</a>
					</div>
					<div class="rank-item">
